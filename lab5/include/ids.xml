<?xml version="1.0" encoding="UTF-8"?>
<nf:module xmlns:nf="http://www.NetFPGA.org/NF2_register_system" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.NetFPGA.org/NF2_register_system NF2_register_system.xsd ">
	<nf:name>ids</nf:name>
	<nf:prefix>ids</nf:prefix>
	<nf:location>udp</nf:location>
	<nf:description>Registers for IDS</nf:description>
	<nf:blocksize>64</nf:blocksize>
	<nf:registers>
		<nf:register>
		  <nf:name>cpu_power</nf:name>
			<nf:description>cpu power signal</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
		<nf:register>
		  <nf:name>host_addr</nf:name>
			<nf:description>address initialization</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
		<nf:register>
		  <nf:name>host_wdata</nf:name>
			<nf:description>data initialization</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
		<nf:register>
		  <nf:name>host_we</nf:name>
			<nf:description>write enable</nf:description>
			<nf:type>generic_software32</nf:type>
		</nf:register>
        <nf:register>
		  <nf:name>host_rdata</nf:name>
			<nf:description>read data mem</nf:description>
			<nf:type>generic_hardware32</nf:type>
		</nf:register>
	</nf:registers>
</nf:module>

